/* 文字尺寸 */
#rule {
  position: fixed;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
}
#rule .content {
    padding: 10rpx 20rpx;
    height: -webkit-calc(100% - 120rpx);
    height: calc(100% - 120rpx);
    overflow: auto;
    color: #333;
    font-size: 28rpx;
}
#rule .btn {
    position: absolute;
    bottom: 0;
    left: 0;
    width: 100%;
    height: 100rpx;
    background: #FA893B;
    color: white;
    font-size: 32rpx;
    text-align: center;
    line-height: 100rpx;
}

