<view id="register"><view class="top-box"><view><text>姓名</text><input type="text" placeholder="请输入姓名" data-event-opts="{{[['input',[['__set_model',['$0','realName','$event',[]],['params']]]]]}}" value="{{params.realName}}" bindinput="__e"/></view><view><text>手机号</text><input type="number" placeholder="请输入手机号" data-event-opts="{{[['input',[['__set_model',['$0','mobile','$event',[]],['params']]]]]}}" value="{{params.mobile}}" bindinput="__e"/></view><view><text>身份证</text><input type="text" placeholder="请输入身份证" data-event-opts="{{[['input',[['__set_model',['$0','idCard','$event',[]],['params']]]]]}}" value="{{params.idCard}}" bindinput="__e"/></view><view><text>车型</text><picker value="{{dataList[index].id}}" range="{{dataList}}" range-key="transportName" data-event-opts="{{[['change',[['typeChange',['$event']]]]]}}" bindchange="__e"><view class="picker">{{''+dataList[index].transportName+''}}</view></picker></view><view><text>车牌号</text><input type="text" placeholder="请输入车牌号" data-event-opts="{{[['input',[['__set_model',['$0','plateNumber','$event',[]],['params']]]]]}}" value="{{params.plateNumber}}" bindinput="__e"/></view><view class="last"><text>认证状态</text><radio-group data-event-opts="{{[['change',[['stateChange',['$event']]]]]}}" bindchange="__e"><label><radio value="1" checked="true" color="#FA893B"></radio>快送</label><label><radio value="2" color="#FA893B"></radio>货运</label><label><radio value="3" color="#FA893B"></radio>快递</label></radio-group></view></view><view class="tel"><view><text>紧急联系人</text><input type="text" placeholder="请输入紧急联系人" data-event-opts="{{[['input',[['__set_model',['$0','emergencyContact','$event',[]],['params']]]]]}}" value="{{params.emergencyContact}}" bindinput="__e"/></view><view class="last"><text>联系人电话</text><input type="number" placeholder="请输入联系人电话" data-event-opts="{{[['input',[['__set_model',['$0','emergencyMobile','$event',[]],['params']]]]]}}" value="{{params.emergencyMobile}}" bindinput="__e"/></view></view><view class="upload"><view><text>身份证正面</text><view data-event-opts="{{[['tap',[['upload',[1]]]]]}}" bindtap="__e"><image src="{{idCardUrlOne}}"></image></view></view><view><text>身份证反面</text><view data-event-opts="{{[['tap',[['upload',[2]]]]]}}" bindtap="__e"><image src="{{idCardUrlTwo}}"></image></view></view><view><text>驾驶证</text><view data-event-opts="{{[['tap',[['upload',[3]]]]]}}" bindtap="__e"><image src="{{driveUrl}}"></image></view></view><view><text>行驶证</text><view data-event-opts="{{[['tap',[['upload',[4]]]]]}}" bindtap="__e"><image src="{{moveUrl}}"></image></view></view></view><view class="submit"><view data-event-opts="{{[['tap',[['submit',['$event']]]]]}}" bindtap="__e">申请入驻</view></view></view>