page {
  height: 100%;
}
#registerSuccess {
  height: -webkit-calc(100% - 80rpx);
  height: calc(100% - 80rpx);
  background: #F9F9F9;
  padding: 40rpx;
  font-size: 28rpx;
  color: #333;
  padding-top: 300rpx;
}
#registerSuccess > view:nth-of-type(1) {
    font-size: 40rpx;
    text-align: center;
}
#registerSuccess > view:nth-of-type(2) {
    padding: 40rpx 0;
    text-indent: 2em;
}
#registerSuccess > view:nth-of-type(3) {
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-pack: center;
    -webkit-justify-content: center;
        -ms-flex-pack: center;
            justify-content: center;
}
#registerSuccess > view:nth-of-type(3) > text {
      border: 1px solid #FA893B;
      -webkit-border-radius: 5px;
              border-radius: 5px;
      padding: 10rpx 40rpx;
      background: white;
}

